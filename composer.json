{
    "name": "ajardin/origami",
    "description": "A PHP toolbox to manage local Docker environments",
    "type": "application",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Jardin",
            "email": "info@ajardin.fr"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "sensio/framework-extra-bundle": "^5.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.3",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "codecov/codecov-php": "dev-master",
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "friendsofphp/php-cs-fixer": "3.0.x-dev",
        "jangregor/phpstan-prophecy": "^0.4.2",
        "liip/functional-test-bundle": "^3.2",
        "phpstan/phpstan": "^0.11.3",
        "phpstan/phpstan-phpunit": "^0.11.2",
        "phpstan/phpstan-symfony": "^0.11.6",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^4.1",
        "sensiolabs/security-checker": "^6.0",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/var-dumper": "^4.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-autoload-dump": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
